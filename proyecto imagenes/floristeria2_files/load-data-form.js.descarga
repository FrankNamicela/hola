let customLinks=document.querySelectorAll('.single-landing ul.list-links a');customLinks.forEach(link=>{link.addEventListener('mousedown',function(e){e.stopPropagation();e.preventDefault();customClick(this)})});async function customClick(element){while(!document.querySelector('#form-popup form')){await new Promise(r=>setTimeout(r,500))}
